{
  "name": "soundvast",
  "version": "1.0.0",
  "description": "Audio Streaming Music Website",
  "author": "Martin Dawson <u1356770@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MartinDawson/SoundVast.git"
  },
  "bugs": {
    "url": "https://github.com/MartinDawson/SoundVast/issues"
  },
  "keywords": [
    "soundvast",
    "audio",
    "video",
    "streaming"
  ],
  "homepage": "https://github.com/MartinDawson/SoundVast",
  "license": "ISC",
  "scripts": {
    "lint": "eslint --fix --ext .jsx,.js wwwroot/components",
    "test": "cross-env BABEL_ENV=test mocha wwwroot/components/**/*.spec.{js,jsx} --opts ./mocha.opts",
    "coverage": "cross-env BABEL_ENV=test nyc mocha wwwroot/components/**/*.spec.{js,jsx} --opts ./mocha.opts",
    "test:watch": "npm test -- -w",
    "packages": "david",
    "packages:update": "david update"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "browserslist": [
    "> 3%",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "edge >= 12"
  ],
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "include": "Components",
    "exclude": [
      "**/*.spec.{js,jsx}"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true,
    "cache": true,
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "aspnet-webpack": "^1.0.29",
    "lodash": "^4.15.0",
    "prop-types": "^15.5.8",
    "react": "^15.3.0",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.3.0",
    "react-jplayer": "^5.0.0",
    "react-jplaylist": "^1.0.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "recompose": "^0.23.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3",
    "xhr2": "^0.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.18.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "david": "^11.0.0",
    "editorconfig": "^0.13.2",
    "enzyme": "^2.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "jsdom": "^11.0.0",
    "less": "^2.7.2",
    "less-loader": "^3.0.0",
    "mocha": "^3.1.2",
    "nyc": "^10.1.2",
    "postcss-loader": "^1.3.3",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "redux-mock-store": "^1.2.3",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
