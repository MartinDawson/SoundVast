{
  "name": "soundvast",
  "version": "1.0.0",
  "description": "Audio Streaming Music Website",
  "author": "Martin Dawson <u1356770@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MartinDawson/SoundVast.git"
  },
  "bugs": {
    "url": "https://github.com/MartinDawson/SoundVast/issues"
  },
  "keywords": [
    "soundvast",
    "audio",
    "video",
    "streaming"
  ],
  "homepage": "https://github.com/MartinDawson/SoundVast",
  "license": "ISC",
  "scripts": {
    "dev": "cross-env ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "lint": "eslint --fix --ext .jsx,.js wwwroot/components",
    "test": "cross-env BABEL_ENV=test mocha wwwroot/components/**/*.spec.{js,jsx} --opts ./mocha.opts",
    "coverage": "cross-env BABEL_ENV=test nyc mocha wwwroot/components/**/*.spec.{js,jsx} --opts ./mocha.opts",
    "test:watch": "npm test -- -w",
    "packages": "david",
    "packages:update": "david update"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "browserslist": [
    "> 3%",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "edge >= 12"
  ],
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "include": "wwwroot/components/**/*.{js,jsx}",
    "exclude": [
      "wwwroot/components/**/*.spec.{js,jsx}"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true,
    "cache": true,
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.18.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "css-modules-require-hook": "^4.0.6",
    "david": "^11.0.0",
    "editorconfig": "^0.13.2",
    "enzyme": "^2.5.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "fetch-mock": "^5.12.1",
    "file-loader": "^0.11.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.0.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "mocha": "^3.1.2",
    "nyc": "^11.0.3",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.2.2",
    "proxyquire": "^1.8.0",
    "react-addons-test-utils": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "redux-mock-store": "^1.2.3",
    "require-hacker": "^3.0.1",
    "style-loader": "^0.18.2",
    "url-search-params-polyfill": "^2.0.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "airbnb-prop-types": "^2.7.0",
    "aspnet-webpack": "^1.0.29",
    "classnames": "^2.2.5",
    "es6-promise": "^4.1.0",
    "fetch-intercept": "^2.2.3",
    "isomorphic-fetch": "^2.2.1",
    "jsmediatags": "^3.6.1",
    "lodash": "^4.17.4",
    "normalizr": "^3.2.3",
    "prop-types": "^15.5.8",
    "react": "^15.3.0",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.3.0",
    "react-dropzone": "^3.13.3",
    "react-html-email": "^1.2.0",
    "react-infinite-scroller": "^1.0.14",
    "react-jplayer": "latest",
    "react-jplayer-utils": "latest",
    "react-jplaylist": "latest",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-svg-loader": "^1.1.1",
    "react-tabs": "^1.1.0",
    "recompose": "^0.23.4",
    "redux": "^3.5.2",
    "redux-form": "^6.7.0",
    "redux-thunk": "^2.2.0",
    "shortid": "^2.2.8",
    "xhr2": "^0.1.4"
  }
}
